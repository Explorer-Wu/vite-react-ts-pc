{
  "name": "vite-react-ts-pc",
  "private": true,
  "version": "1.0.0",
  "description": "vite 构建react+ts pc端单页面应用模版",
  "scripts": {
    "start": "vite --force",
    "dev:local": "vite --mode _local",
    "testing": "vite --mode testing",
    "staging": "vite --mode staging",
    "build:testing": "tsc && vite build --mode testing",
    "build:staging": "tsc && vite build --mode staging",
    "build": "tsc -p tsconfig.prod.json && vite build",
    "build:noTypeCheck": "vite build",
    "preview": "vite preview",
    "test:unit": "jest",
    "test:watch": "jest --env=testing --watch",
    "test:nc": "npm run test:watch -- --no-cache",
    "prepare": "husky install",
    "commit": "commit",
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --cache ./src",
    "analyze": "source-map-explorer 'dist/assets/*.js'"
  },
  "dependencies": {
    "antd": "^4.21.0",
    "axios": "^0.27.2",
    "camelcase": "^6.3.0",
    "dayjs": "^1.11.7",
