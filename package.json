    "immer": "^9.0.19",
    "lodash": "^4.17.21",
    "lru-cache": "^7.18.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
